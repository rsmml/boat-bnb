<%= simple_form_for @boat do |f|%>
  <%= f.input :name %>
  <%= f.input :description %>
  <%= f.input :capacity %>
  <%= f.input :location %>
  <%= f.input :price %>
  <%= f.input :type %>
  <%= f.input :photo, as: :file %>
  <!-- we must make the fields allow selection Select2 -->
  <%= f.submit %>
  <% end %>
