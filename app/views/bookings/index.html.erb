<div class="container">
  <h2>Here are your bookings</h2>
  <% @bookings.each do |booking| %>
    <h3><%= link_to booking.boat.name, booking_path(booking) %> <%=  %></h3>
    <p><%= booking.status %></p>
    <p><%= booking.checkin %> -- <%= booking.checkout %> </p>
    <p><%= booking.booking_price %></p>
  <% end %>
</div>
